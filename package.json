{
  "name": "react-diagram-schema",
  "version": "0.3.8",
  "description": "CLI tool that parses React components and generates a structured JSON schema capturing component hierarchy, state, props, and context",
  "main": "./src/build-schema.js",
  "scripts": {
    "test": "npx jest ./tests/",
    "lint": "npx eslint . --fix",
    "format": "npx prettier . --write",
    "commit": "./scripts/generate-commit-msg.sh"
  },
  "exports": {
    ".": "./src/build-schema.js",
    "./cli-core": "./src/cli-core.js"
  },
  "bin": {
    "react-diagram-schema": "src/cli.js",
    "build-schema": "src/cli.js"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "ROADMAP.md",
    "CONTRIBUTING.md",
    "assets/readme-md-diagram-preview.png",
    "assets/contributing-md-diagram-preview.png",
    "src",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmiraBasyouni/react-diagram-schema.git"
  },
  "keywords": [
    "react",
    "babel",
    "jsx",
    "typescript",
    "ast",
    "parser",
    "schema",
    "component-diagram",
    "component-analysis",
    "code-visualization",
    "developer-tooling"
  ],
  "author": "Amira Basyouni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AmiraBasyouni/react-diagram-schema/issues"
  },
  "homepage": "https://github.com/AmiraBasyouni/react-diagram-schema#readme",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "eslint": "^9.30.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "prettier": "3.6.2"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "typescript": "^5.9.2"
  }
}
